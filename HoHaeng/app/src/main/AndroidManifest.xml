<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hamseong.hohaeng">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <queries>
        <package android:name="com.skt.skaf.l001mtm091" />
        //빨간색 티맵
        <package android:name="com.skt.tmap.ku" />
        //초록색 티맵
    </queries>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HoHaeng"
        android:usesCleartextTraffic="true">

        <activity android:name=".view.AlreadyMapView" />
        <activity android:name=".view.TMapViewView">
        </activity>
        <activity android:name=".view.MyView" />
        <activity android:name=".view.MapViewView" />

        <activity android:exported="true" android:name=".view.MainActivity">
            <intent-filter>

                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>

            </intent-filter>
        </activity>
        <activity android:name=".view.HomeActivity"/>

        <activity android:name=".view.SpecialtiesActivity"/>

        <activity android:name=".view.SelectActivity"/>

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="96c8e7c0f37e4ef21919888dc6994ed3" />
    </application>

</manifest>